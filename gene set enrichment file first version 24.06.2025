#sonraki ekledigim gen setlerinde, yukaridaki gene_set_names'e de bunun adini ekleyen bir fonksyion yazacagim

def compare_gene_sets(query_genes, target_genes):
    query_upper = set(str(g).upper() for g in query_genes if g is not None)
    target_upper = set(str(g).upper() for g in target_genes if g is not None)
    common = query_upper.intersection(target_upper)

    print(f"Ortak gen sayısı: {len(common)}")
    print("Ortak genler:", sorted(common))

